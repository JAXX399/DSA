#include <vector>
#include <map>  
#include <iostream>
using namespace std;


class Solution{
public:
    int subarraysWithXorK(vector<int> &nums, int k) {
        int xr=0;
        map<int,int> mpp;
        int cnt=0;
        for(int i =0;i<nums.size();i++){
            xr= xr^nums[i];
            int x = xr^k;
            cnt=cnt+mpp[x];
            mpp[xr]++;
            
        }
        
    }
};

int main() {
    Solution sol;
    vector<int> nums = {4, 2, 2, 6, 4};
    int k = 6;
    
    int result = sol.subarraysWithXorK(nums, k);
    
    cout << "Number of subarrays with XOR equal to " << k << ": " << result << endl;
    
    return 0;
}